  },
  "noise": {
    "min_y": -64,
    "height": 384,
    "size_horizontal": 4,
    "size_vertical": 4
  },
  "noise_router": {
    "barrier": -1,
    "fluid_level_floodedness": -1,
    "fluid_level_spread": -1,
    "lava": -1,
    "temperature": {
      "type": "hydrol:store_temperature",
      "input": "hydrol:test/temperature"
    },
    "vegetation": "hydrol:test/detail_noise",
    "erosion": 0.2,
    "continents": {
      "type": "minecraft:add",
      "argument1": -0.34,
      "argument2": {
        "type": "minecraft:abs",
        "argument": "hydrol:test/continentalness"
      }
    },
    "depth": 0.26,
    "ridges": "hydrol:test/ridges",
    "initial_density_without_jaggedness": {
      "type": "minecraft:range_choice",
      "input": {
        "type": "minecraft:y_clamped_gradient",
        "from_y": 15,
        "to_y": 16,
        "from_value": 0,
        "to_value": 1
      },
      "min_inclusive": 0,
      "max_exclusive": 0.1,
      "when_in_range": 1,
      "when_out_of_range": -1
    },
    "final_density": "hydrol:test/final_density",
    "vein_toggle": 0,
    "vein_ridged": 0,
    "vein_gap": 0
  },
  "spawn_target": [],
  "surface_rule": {
    "type": "minecraft:sequence",
    "sequence": [
      {
        "type": "minecraft:condition",
        "if_true": {
          "type": "minecraft:vertical_gradient",
          "random_name": "minecraft:bedrock_floor",
          "true_at_and_below": {
            "above_bottom": 0
          },
          "false_at_and_above": {
            "above_bottom": 5
          }
        },
        "then_run": {
          "type": "minecraft:block",
          "result_state": {
            "Name": "minecraft:bedrock"
          }
        }
      },
      {
        "type": "minecraft:condition",
        "if_true": {
          "type": "minecraft:stone_depth",
          "offset": 0,
          "surface_type": "floor",
          "add_surface_depth": false,
          "secondary_depth_range": 0
        },
        "then_run": {
          "type": "minecraft:block",
          "result_state": {
            "Name": "minecraft:rooted_dirt"
          }
        }
      },
      {
        "type": "minecraft:condition",
        "if_true": {
          "type": "minecraft:stone_depth",
          "offset": 2,
          "surface_type": "floor",
          "add_surface_depth": false,
          "secondary_depth_range": 0
        },
        "then_run": {
          "type": "minecraft:block",
          "result_state": {
            "Name": "minecraft:dirt"
          }
        }
      },
      {
        "type": "minecraft:condition",
        "if_true": {
          "type": "minecraft:biome",
          "biome_is": [
            "minecraft:desert"
          ]
        },
        "then_run": {
          "type": "minecraft:block",
          "result_state": {
            "Name": "minecraft:sandstone"
          }
        }
      },
      {
        "type": "minecraft:condition",
        "if_true": {
          "type": "minecraft:biome",
          "biome_is": [
            "minecraft:savanna_plateau",
            "minecraft:savanna"
          ]
        },
        "then_run": {
          "type": "minecraft:block",
          "result_state": {
            "Name": "minecraft:red_sandstone"
          }
        }
      },
      {
        "type": "minecraft:condition",
        "if_true": {
          "type": "hydrol:temperature",
          "threshold": -0.8,
          "greater": false
        },
        "then_run": {
          "type": "minecraft:block",
          "result_state": {
            "Name": "minecraft:deepslate"
          }
        }
      },
      {
        "type": "minecraft:condition",
        "if_true": {
          "type": "hydrol:temperature",
          "threshold": 0.8,
          "greater": true
        },
        "then_run": {
          "type": "minecraft:bandlands"
        }
      },
      {
        "type": "minecraft:condition",
        "if_true": {
          "type": "hydrol:temperature",
          "threshold": 0.4,
          "greater": true
        },
        "then_run": {
          "type": "minecraft:block",
          "result_state": {
            "Name": "minecraft:calcite"
          }
        }
      }
    ]
  }
}